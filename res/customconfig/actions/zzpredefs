    hotplug_sleep="1"
    freq_limit_sleep="800000"
    freq_step_sleep="1"
    fast_scaling_sleep="0"
    zzmoove_predef="2"
    ;;
  3)
    sampling_rate="120000"
    sampling_down_factor="1"
    up_threshold="95"
    down_threshold="40"
    ignore_nice_load="0"
    freq_step="10"
    smooth_up="75"
