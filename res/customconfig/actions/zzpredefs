echo `sampling_down_factor=$sampling_down_factor` >> $DEFAULT_PROFILE
echo `up_threshold=$up_threshold` >> $DEFAULT_PROFILE
echo `down_threshold=$down_threshold` >> $DEFAULT_PROFILE
echo `ignore_nice_load=$ignore_nice_load` >> $DEFAULT_PROFILE
echo `freq_step=$freq_step` >> $DEFAULT_PROFILE
echo `smooth_up=$smooth_up` >> $DEFAULT_PROFILE
echo `freq_limit=$freq_limit` >> $DEFAULT_PROFILE
echo `fast_scaling=$fast_scaling` >> $DEFAULT_PROFILE
echo `up_threshold_hotplug1=$up_threshold_hotplug1` >> $DEFAULT_PROFILE
echo `up_threshold_hotplug2=$up_threshold_hotplug2` >> $DEFAULT_PROFILE
echo `up_threshold_hotplug3=$up_threshold_hotplug3` >> $DEFAULT_PROFILE
echo `down_threshold_hotplug1=$down_threshold_hotplug1` >> $DEFAULT_PROFILE
echo `down_threshold_hotplug2=$down_threshold_hotplug2` >> $DEFAULT_PROFILE
echo `down_threshold_hotplug3=$down_threshold_hotplug3` >> $DEFAULT_PROFILE
